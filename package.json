    "babel-jest": "~21.2.0",
    "coveralls": "~3.0.0",
    "eslint": "~4.12.0",
    "eslint-plugin-muriki": "~1.0.0",
    "husky": "~0.14.3",
    "jest": "~21.2.1",
    "lint-staged": "~5.0.0"
  },
  "lint-staged": {
    "*.js": "eslint"
  }
